  "main": "./core/src/extension.js",
    "contributes": {
    "configuration": {
      "title": "Uni Header",
      "properties": {
        "tsiheader.username": {
          "type": "string",
          "description": "Username to use in Uni headers",
          "scope": "resource"
        },
        "tsiheader.email": {
          "type": "string",
          "description": "Email address to use in Uni headers",
          "scope": "resource"
        },
        "tsiheader.studyMode.enableSounds": {
          "type": "boolean",
          "default": false,
          "description": "Enable audio notifications for timer events"
        },
        "tsiheader.studyMode.workDuration": {
          "type": "number",
          "default": 25,
          "minimum": 1,
          "maximum": 120,
          "description": "Work session duration in minutes (1-120)"
        },
        "tsiheader.studyMode.shortBreakDuration": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 60,
          "description": "Short break duration in minutes (1-60)"
        },
        "tsiheader.studyMode.longBreakDuration": {
          "type": "number",
          "default": 15,
          "minimum": 1,
          "maximum": 120,
          "description": "Long break duration in minutes (1-120)"
        },
        "tsiheader.studyMode.sessionsBeforeLongBreak": {
          "type": "number",
          "default": 4,
          "minimum": 1,
          "maximum": 10,
          "description": "Number of work sessions before a long break (1-10)"
        },
        "tsiheader.calendar.importUrl": {
          "type": "string",
          "description": "URL for importing calendar data from external source",
          "scope": "resource"
        },
        "tsiheader.notifications.enableEmail": {
          "type": "boolean",
          "default": false,
          "description": "Enable email notifications for upcoming events and deadlines",
          "scope": "resource"
        },
        "tsiheader.notifications.emailService": {
          "type": "string",
          "enum": ["smtp"],
          "enumDescriptions": [
            "Custom SMTP server"
          ],
          "description": "Email service provider for notifications",
          "scope": "resource"
        },
        "tsiheader.notifications.emailAddress": {
          "type": "string",
          "description": "Email address to send notifications to",
          "scope": "resource"
        },
        "tsiheader.notifications.advanceNotice": {
          "type": "number",
          "default": 24,
          "minimum": 1,
          "maximum": 168,
          "description": "Hours in advance to send notifications (1-168 hours)",
          "scope": "resource"
        },
        "tsiheader.notifications.smtpHost": {
          "type": "string",
          "description": "SMTP server hostname",
          "scope": "resource"
        },
        "tsiheader.notifications.smtpPort": {
          "type": "number",
          "default": 587,
          "description": "SMTP server port",
          "scope": "resource"
        },
        "tsiheader.notifications.smtpUser": {
          "type": "string",
          "description": "SMTP username",
          "scope": "resource"
        },
        "tsiheader.notifications.smtpPassword": {
          "type": "string",
          "description": "SMTP password",
          "scope": "resource"
        },
        "tsiheader.customHeader.enableCustomHeader": {
          "type": "boolean",
          "default": false,
          "description": "Enable custom header with your institution branding instead of TSI default",
          "scope": "resource"
        },
        "tsiheader.customHeader.institutionName": {
          "type": "string",
          "default": "Transport and Telecommunication Institute - Riga, Latvia",
          "description": "Your institution/university name to display in custom headers",
          "scope": "resource"
        },
        "tsiheader.customHeader.institutionUrl": {
          "type": "string",
          "default": "https://tsi.lv",
          "description": "Your institution/university website URL to display in custom headers",
          "scope": "resource"
        }
      }
    },
